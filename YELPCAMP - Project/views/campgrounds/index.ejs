<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Campgrounds</title>
  </head>
  <body>
    <h1>All Campgrounds!</h1>

    <div>
      <a href="/campgrounds/new" target="_blank">Add new Campground</a>
    </div>

    <ul>
      <% for(let campground of allCamps) { %>
      <li>
        <a href="/campgrounds/<%= campground._id %>" target="_blank"><%= campground.title %></a>
      </li>
      <% } %>
    </ul>
  </body>
</html>
